{
    "presets": [
      ["@babel/env", {
        "modules": false,
        "targets": {
          "browsers": "> 1%",
          "uglify": true
        },
        "useBuiltIns": true
      }]
    ],
  
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-proposal-object-rest-spread",
      ["@babel/plugin-proposal-class-properties", { "spec": true }]
    ]
}